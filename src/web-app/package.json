{
  "name": "user-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "user-client/node_modules/.bin/vite",
    "build": "user-client/node_modules/.bin/vite build",
    "preview": "user-client/node_modules/.bin/vite preview",
    "test": "user-client/node_modules/.bin/jest --coverage",
    "sonar": "node sonar-project.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "axios": "^0.26.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "jest": "^27.5.1",
    "jest-sonar-reporter": "^2.0.0",
    "sonar-scanner": "^3.1.0",
    "sonarqube-scanner": "^2.8.1",
    "vite": "^2.9.0"
  }
}
